{% extends 'base.html.twig' %}
{% import 'macro/breadcrumb.html.twig' as breadcrumb %}
{% import 'macro/action.html.twig' as action %}

{% set _title = 'Traslado' %}
{% set _main_title = 'Mostrar' %}
{% set _section =  'traslado_index'%}

{% block _breadcrumb %}
    {{ breadcrumb.show2(_title, _section, 'Mostrar') }}
{% endblock %}

{% block _main %}
    {{ action.tbl_start() }}
        {{ action.tbl_tr('Fecha Salida',traslado.nombre) }}
        {{ action.tbl_tr('Numero Guia',traslado.numeroGuia) }}
        {{ action.tbl_tr('Almacen Origen',traslado.almacenOrigen) }}
        {{ action.tbl_tr('Almacen Destino',traslado.almacenDestino) }}
        
        {% include 'crud/_show_info.html.twig' with {'entity': traslado} %}
    {{ action.tbl_end() }}

{% endblock %}

{% block _main_footer %}
    {% include 'crud/_show_links.html.twig' with {'route_base':'traslado','entity': traslado} %}
{% endblock %}

{% block _main_tools %}
    {% include 'crud/_tools_ico.html.twig' %}
{% endblock %}

{% block _body_tools %}
    {% include 'crud/_show_links_ico.html.twig' with {'route_base':'traslado','entity': traslado} %}
{% endblock %}

{% block _body_tools_ico %}
    {% include 'crud/_show_links_tool.html.twig' with {'route_base':'traslado','entity': traslado} %}
{% endblock %}