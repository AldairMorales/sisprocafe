{% import 'macro/action.html.twig' as action %}

{{ action.frm_start(form) }}
    {{ action.frm_column(
        action.frm_column_field(form.fechaSalida),
        action.frm_column_field(form.numeroGuia)
    ) }}
    {{ action.frm_column(
        action.frm_column_field_select(form.almacenOrigen),
        action.frm_column_field_select(form.almacenDestino)
    ) }}
    {{ action.frm_column(
        action.frm_column_field_collection(form.detalleTraslado,'','','',2),
        null,
        null,null,1 
    ) }}
    {{ action.frm_column(
        action.frm_column_btn(button_label|default('Guardar')),
        null,
        null,
        null,
        2
    ) }}
{{ action.frm_end(form) }}