{% extends 'base.html.twig' %}
{% import 'macro/breadcrumb.html.twig' as breadcrumb %}
{% import 'macro/action.html.twig' as action %}

{% set _title = 'Analisis Fisico' %}
{% set _main_title = 'Mostrar' %}
{% set _section =  'analisisFisico_index'%}

{% block _breadcrumb %}
    {{ breadcrumb.show2(_title, _section, 'Mostrar') }}
{% endblock %}

{% block _main %}
    {{ action.tbl_start() }}
            {{ action.tbl_tr('fecha', analisisFisico.fecha) }}
            {{ action.tbl_tr('certificacion', analisisFisico.certificacion) }}
            {{ action.tbl_tr('muestra', analisisFisico.muestra) }}
            {{ action.tbl_tr('exportable', analisisFisico.exportable) }}
            {{ action.tbl_tr('bola', analisisFisico.bola) }}
            {{ action.tbl_tr('segunda', analisisFisico.segunda) }}
            {{ action.tbl_tr('casara', analisisFisico.cascara) }}
            {{ action.tbl_tr('descripcion', analisisFisico.descripcion) }}
            {{ action.tbl_tr('Activo', analisisFisico.activo, 'text-center') }}
        
        {% include 'crud/_show_info.html.twig' with {'entity': analisisFisico} %}
    {{ action.tbl_end() }}

{% endblock %}

{% block _main_footer %}
    {% include 'crud/_show_links.html.twig' with {'route_base':'analisisFisico','entity': analisisFisico} %}
{% endblock %}

{% block _main_tools %}
    {% include 'crud/_tools_ico.html.twig' %}
{% endblock %}

{% block _body_tools %}
    {% include 'crud/_show_links_ico.html.twig' with {'route_base':'analisisFisico','entity': analisisFisico} %}
{% endblock %}

{% block _body_tools_ico %}
    {% include 'crud/_show_links_tool.html.twig' with {'route_base':'analisisFisico','entity': analisisFisico} %}
{% endblock %}