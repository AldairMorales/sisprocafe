{% import 'macro/action.html.twig' as action %}

{{ action.frm_start(form) }}
    {{ action.frm_column(
        action.frm_column_field_date(form.fecha),
        action.frm_column_field_select(form.certificacion)
    ) }}
    {{ action.frm_column(
        action.frm_column_field_select(form.acopio),
        action.frm_column_field_select(form.muestra)
        
    ) }}
    {{ action.frm_column(
        action.frm_column_line()
    )}}
    
    {{ action.frm_column(
        action.frm_column_field(form.exportable),
        action.frm_column_field(form.exportableP)
    )}}
    {{ action.frm_column(
        action.frm_column_field(form.bola),
        action.frm_column_field(form.bolaP)
    ) }}
    {{ action.frm_column(
        action.frm_column_field(form.segunda),
        action.frm_column_field(form.segundaP)
    ) }}
    {{ action.frm_column(
        action.frm_column_field(form.cascara),
        action.frm_column_field(form.cascaraP)
    ) }}
    {{ action.frm_column(
        action.frm_column_field_number(form.humedad),
        action.frm_column_field(form.descripcion)
    ) }}
    
    {{ action.frm_column(
        action.frm_column_btn(button_label|default('Guardar')),
        null,
        null,
        null,
        2
    )}}
{{ action.frm_end(form) }}