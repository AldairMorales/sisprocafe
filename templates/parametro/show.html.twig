{% extends 'base.html.twig' %}
{% import 'macro/breadcrumb.html.twig' as breadcrumb %}
{% import 'macro/action.html.twig' as action %}

{% set _title = 'Parametro' %}
{% set _main_title = 'Mostrar' %}
{% set _section =  'parametro_index'%}

{% block _breadcrumb %}
    {{ breadcrumb.show2(_title, _section, 'Mostrar') }}
{% endblock %}

{% block _main %}
    {{ action.tbl_start() }}
    {{ action.tbl_tr('Padre',parametro.padre) }}
    {{ action.tbl_tr('Nombre',parametro.nombre) }}
    {{ action.tbl_tr('Alias',parametro.alias) }}
    {{ action.tbl_tr('Valor',parametro.valor) }}
    {% include 'crud/_show_info.html.twig' with {'entity': parametro} %}
    {{ action.tbl_end() }}

{% endblock %}

{% block _main_footer %}
    {% include 'crud/_show_links.html.twig' with {'route_base':'parametro','entity': parametro} %}
{% endblock %}

{% block _main_tools %}
    {% include 'crud/_tools_ico.html.twig' %}
{% endblock %}

{% block _body_tools %}
    {% include 'crud/_show_links_ico.html.twig' with {'route_base':'parametro','entity': parametro} %}
{% endblock %}

{% block _body_tools_ico %}
    {% include 'crud/_show_links_tool.html.twig' with {'route_base':'parametro','entity': parametro} %}
{% endblock %}