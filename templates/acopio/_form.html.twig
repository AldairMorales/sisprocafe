{% import 'macro/action.html.twig' as action %}
{{ action.frm_start(form) }}
    {{ action.frm_column(
        action.frm_column_field_select(form.periodo),
        action.frm_column_field_select(form.fecha)
    ) }}
    {{ action.frm_column(
        action.frm_column_field_select(form.socio),
        action.frm_column_field_select(form.certificacion)
    ) }}
    {{ action.frm_column(
        action.frm_column_field(form.almacen),
        action.frm_column_field(form.ticket)
    ) }}
    {{ action.frm_column(
        action.frm_column_field(form.pesoBruto),
        action.frm_column_field(form.cantidad)
    ) }}
    {{ action.frm_column(
        action.frm_column_field(form.taraPorSaco),
        action.frm_column_field(form.taraDeSaco)
    ) }}
    {{ action.frm_column(
        action.frm_column_field(form.pesoQuintales),
        action.frm_column_field(form.pesoNeto)
    ) }}
    {{ action.frm_column(
        action.frm_column_field(form.observaciones),
        null, null, null, 2
    ) }}
    {{ action.frm_column(
        action.frm_column_btn(button_label|default('Guardar')),
        null,
        null,
        null,
        2
    ) }}
{{ action.frm_end(form) }}