{% import 'macro/action.html.twig' as action %}
{{ action.frm_start(form) }}
    {{ action.frm_column(
        action.frm_column_field_select(form.periodo),
        action.frm_column_field_date(form.fecha)
    ) }}
    {{ action.frm_column(
        action.frm_column_field_select(form.socio),
        action.frm_column_field_select(form.certificacion)
    ) }}
    {{ action.frm_column(
        action.frm_column_field_select(form.almacen),
        action.frm_column_field(form.tikect)
    ) }}
    {{ action.frm_column(
        action.frm_column_field_add_select(form.pesoBruto),
        action.frm_column_field_add_select(form.cantidad)
    ) }}
    {{ action.frm_column(
        action.frm_column_field(form.taraPorSaco),
        action.frm_column_field_add_select(form.taraDeSacos)
    ) }}
    {{ action.frm_column(
        action.frm_column_field_add_select(form.pesoQuintales),
        action.frm_column_field_add_select(form.pesoNeto)
    ) }}
    {{ action.frm_column(
        action.frm_column_field(form.observaciones),
        null, null, null, 2
    ) }}
    {{ action.frm_column(
        action.frm_column_btn(button_label|default('Guardar')),
        null,
        null,
        null,
        2
    ) }}
{{ action.frm_end(form) }}