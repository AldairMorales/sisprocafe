{% import 'macro/action.html.twig' as action %}
{{ action.frm_start(form) }}
    {{ action.frm_column(
        action.frm_column_field_select(form.acciones),
        null, null, null, 2
    ) }}
    {{ action.frm_column(
        action.frm_column_field_add_select(form.tara),
        action.frm_column_field_add_select(form.humedadInicial)
    ) }}
    {{ action.frm_column(
        action.frm_column_field(form.muestra),
        action.frm_column_field(form.unidadMedida)
    ) }}
    {{ action.frm_column(
        action.frm_column_field(form.envase),
        action.frm_column_field(form.moneda)
    ) }}
    {{ action.frm_column(
        action.frm_column_btn(button_label|default('Guardar')),
        null,
        null,
        null,
        2
    ) }}
{{ action.frm_end(form) }}