{% extends 'base.html.twig' %}
{% import 'macro/breadcrumb.html.twig' as breadcrumb %}
{% import 'macro/action.html.twig' as action %}

{% set _title = 'DetallePeriodo' %}
{% set _main_title = 'Mostrar' %}
{% set _section =  'detalle_periodo_index'%}

{% block _breadcrumb %}
    {{ breadcrumb.show2(_title, _section, 'Mostrar') }}
{% endblock %}

{% block _main %}
    {{ action.tbl_start() }}
    {{ action.tbl_tr('Acciones',detalle_periodo.acciones) }}
    {{ action.tbl_tr('Tara',detalle_periodo.tara) }}
    {{ action.tbl_tr('Humedad',detalle_periodo.humedad) }}
    {{ action.tbl_tr('Muestra',detalle_periodo.muestra) }}
    {{ action.tbl_tr('UnidadMedida',detalle_periodo.unidadMedida) }}
    {% include 'crud/_show_info.html.twig' with {'entity': detalle_periodo} %}
    {{ action.tbl_end() }}

{% endblock %}

{% block _main_footer %}
    {% include 'crud/_show_links.html.twig' with {'route_base':'detalle_periodo','entity': detalle_periodo} %}
{% endblock %}

{% block _main_tools %}
    {% include 'crud/_tools_ico.html.twig' %}
{% endblock %}

{% block _body_tools %}
    {% include 'crud/_show_links_ico.html.twig' with {'route_base':'detalle_periodo','entity': detalle_periodo} %}
{% endblock %}

{% block _body_tools_ico %}
    {% include 'crud/_show_links_tool.html.twig' with {'route_base':'detalle_periodo','entity': detalle_periodo} %}
{% endblock %}