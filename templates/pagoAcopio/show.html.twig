{% extends 'base.html.twig' %}
{% import 'macro/breadcrumb.html.twig' as breadcrumb %}
{% import 'macro/action.html.twig' as action %}

{% set _title = 'Acopio' %}
{% set _main_title = 'Mostrar' %}
{% set _section =  'pagoAcopio_index'%}

{% block _breadcrumb %}
    {{ breadcrumb.show2(_title, _section, 'Mostrar') }}
{% endblock %}

{% block _main %}
    {{ action.tbl_start() }}
    {{ action.tbl_tr('Fecha',pagoAcopio.fecha) }}
    {{ action.tbl_tr('Ticket',pagoAcopio.acopio) }}
    {{ action.tbl_tr('Precio Base',pagoAcopio.precioBase) }}
    {{ action.tbl_tr('Precio Final',pagoAcopio.precioFinal) }}
    {{ action.tbl_tr('Descripcion',pagoAcopio.descripcion) }}
    {% include 'crud/_show_info.html.twig' with {'entity': pagoAcopio} %}
    {{ action.tbl_end() }}

{% endblock %}

{% block _main_footer %}
    {% include 'crud/new_show_links.html.twig' with {'route_base':'pagoAcopio','entity': pagoAcopio} %}
{% endblock %}

{% block _main_tools %}
    {% include 'crud/_tools_ico.html.twig' %}
{% endblock %}


{% block _body_tools_ico %}
    {% include 'crud/_show_links_tool.html.twig' with {'route_base':'pagoAcopio','entity': pagoAcopio} %}
{% endblock %}